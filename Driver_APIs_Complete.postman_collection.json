      "name": "üîê Driver Authentication",
      "item": [
        {
          "name": "Driver Signup",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (responseCode.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('driver_token', response.token);",
                  "    pm.collectionVariables.set('driver_id', response.driver.id);",
                  "    pm.test('Driver registered successfully', function () {",
                  "        pm.expect(response.token).to.exist;",
                  "        pm.expect(response.driver).to.exist;",
                  "    });",
                  "} else {",
                  "    pm.test('Registration failed', function () {",
                  "        pm.expect(responseCode.code).to.not.equal(201);",
                  "    });",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"name\": \"Rajesh Kumar Singh\",\n    \"email\": \"rajesh.driver@example.com\",\n    \"phone\": \"+919876543210\",\n    \"alternatePhone\": \"+919876543211\",\n    \"password\": \"driver123\",\n    \"address\": {\n        \"street\": \"123 Driver Colony\",\n        \"city\": \"Mumbai\",\n        \"state\": \"Maharashtra\",\n        \"pincode\": \"400001\",\n        \"country\": \"India\"\n    },\n    \"dateOfBirth\": \"1985-05-15\",\n    \"licenseNumber\": \"MH02DL987654321\",\n    \"licenseExpiry\": \"2028-05-15\",\n    \"licenseType\": \"Light Motor Vehicle\",\n    \"experience\": 10,\n    \"emergencyContact\": {\n        \"name\": \"Sunita Singh\",\n        \"phone\": \"+919876543212\",\n        \"relation\": \"Wife\"\n    },\n    \"languages\": [\"English\", \"Hindi\", \"Marathi\"],\n    \"specializations\": [\"City Tours\", \"Airport Transfer\", \"Outstation\"],\n    \"vehicleDetails\": {\n        \"make\": \"Maruti Suzuki\",\n        \"model\": \"Swift Dzire\",\n        \"year\": 2022,\n        \"color\": \"White\",\n        \"plateNumber\": \"MH02AB9876\",\n        \"fuelType\": \"Petrol\",\n        \"capacity\": 4,\n        \"features\": [\"AC\", \"GPS\", \"Music System\"]\n    }\n}"
            },
